{
    "name": "@purinton/cassandra",
    "type": "module",
    "version": "1.0.1",
    "description": "A modern, multi-language Discord bot for creative games, drawing prompts, and more. Features modular commands, event handlers, and easy customization.",
    "main": "cassandra.mjs",
    "scripts": {
        "start": "node cassandra.mjs",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/cassandra.git"
    },
    "keywords": [
        "discord",
        "discord-bot",
        "prompt",
        "drawing",
        "game",
        "openai",
        "localization",
        "multi-language",
        "nodejs",
        "cassandra",
        "purinton"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/cassandra/issues"
    },
    "homepage": "https://github.com/purinton/cassandra#readme",
    "devDependencies": {
        "jest": "^30.0.2"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/common": "^1.0.16",
        "@purinton/discord": "^1.0.11",
        "@purinton/mysql": "^1.0.9",
        "@purinton/openai": "^1.0.1",
        "dotenv": "^16.5.0"
    }
}